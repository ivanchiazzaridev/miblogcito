---
// Importamos Layout Base
import BaseLayout from '../layouts/BaseLayout.astro';

// importamos estilos ??
// import '../styles/mdPostLayoutStyle.css';


const { frontmatter } = Astro.props;
---
<BaseLayout pageTitle={frontmatter.title} >
    <!-- <h1>{frontmatter.title}</h1> -->
    <p>Por algun motivo esto parece ser el "contenido principal"</p>
    <p>Publicado el: {frontmatter.pubDate.toString().slice(0,10)}</p>
    <p>Escrito por {frontmatter.author}</p>
    <img class="astro-logo" src={frontmatter.image.url} width="300" alt={frontmatter.image.alt} />
    <slot />
</BaseLayout>
    